#include <stdio.h>
#include <stdlib.h>

//Total of numbers in an array that takes from user

int main()
{
int i,n, total;
int array[100];

 printf("How much elements are there in the array? : ");
 scanf("%d",&n);
 
  for(i=0; i<n;i++)
{
 printf("Please enter the %d. element : ", i);
 scanf("%d", &array[i]);
}

total=0;
  for(i=0;i<n;i++)
    total = total + array[i];
 printf("Total of numbers = %d",total);
    return 0;
}
